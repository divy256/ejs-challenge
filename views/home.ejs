<%-include("partials/header")-%>

<h1>Home</h1>
<p> <%=StartingContent%> </p>

<p>
	<% newPost.forEach(function(post){%>
	<h1><%=post.title%></h1>
	<% var str=post.content.substring(0,100);%>
	<% str=str+"...";%>
	<p><%= str%>
	   <a href="/post/<%=post.title%>">Read More</a>
	</p>
	
<% }); %>

	
</p>
<%-include("partials/footer")-%>